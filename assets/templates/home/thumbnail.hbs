  <div class="thumbnail">
	<a href = '/video/city/{{objectId}}'>
		{{#if vimeo}}
		<img src='{{image_url}}'  width="100%" height="250px">
		{{/if}}
		{{#if youtube}}
		<img src='http://img.youtube.com/vi/{{VideoID}}/0.jpg' width="100%">
		{{/if}}	
		<h4 class="thumbText">{{city_name}}<br>{{country_name}}</h4>
		<div class="play_button">
		Play
		</div>
  	<p class="by">By {{author}}</p>

	</a>
</div>
